<% if signed_in? %>
	
	<script type="text/javascript" src="http://maps.google.com/maps/api/js?v=3.2&amp;sensor=false&amp;region=pl"></script>                                          

	<script type="text/javascript">   
	        function mapaStart()   
	        {   
	            var wspolrzedne = new google.maps.LatLng(53.41935400090768,14.58160400390625);
	            var opcjeMapy = {
				  scrollwheel: false,
	              zoom: 10,
	              center: wspolrzedne,
	              mapTypeId: google.maps.MapTypeId.ROADMAP
	            };
	            var mapa = new google.maps.Map(document.getElementById("map"), opcjeMapy); 
	        }   
	        </script>
	
	<div id="map"></div>
	
	<script type="text/javascript">
	mapaStart();
	</script>

